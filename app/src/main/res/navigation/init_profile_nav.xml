<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/register_nav"
    app:startDestination="@id/inputProfileFragment">

    <fragment
        android:id="@+id/selectedRoleFragment"
        android:name="com.hieuminh.jobfinder.views.fragments.SelectedRoleFragment"
        android:label="fragment_selected_role"
        tools:layout="@layout/fragment_selected_role">

        <action
            android:id="@+id/action_selectedRoleFragment_to_selectJobFragment"
            app:destination="@id/selectJobFragment"
            app:popUpTo="@id/selectedRoleFragment"
            app:popUpToInclusive="true" />

    </fragment>

    <fragment
        android:id="@+id/selectJobFragment"
        android:name="com.hieuminh.jobfinder.views.fragments.SelectJobFragment"
        android:label="fragment_select_job"
        tools:layout="@layout/fragment_select_job" />
    <fragment
        android:id="@+id/inputProfileFragment"
        android:name="com.hieuminh.jobfinder.views.fragments.InputProfileFragment"
        android:label="fragment_input_profile"
        tools:layout="@layout/fragment_input_profile" />

</navigation>

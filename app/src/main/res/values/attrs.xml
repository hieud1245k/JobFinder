<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="HeaderView">
        <attr name="title" format="string" />
        <attr name="icon_src" format="integer" />
    </declare-styleable>

    <declare-styleable name="BaseInputDataView">
        <attr name="view_title" format="string" />
        <attr name="text" format="string" />
        <attr name="hint" format="string" />
        <attr name="suffixSrc" format="integer" />
    </declare-styleable>

    <declare-styleable name="EditView">
        <attr name="suffixIconSrc" format="reference|color" />
    </declare-styleable>

    <declare-styleable name="SettingItemView">
        <attr name="android:text" format="string" />
        <attr name="prefixIconSrc" format="reference" />
        <attr name="prefixBackgroundTint" format="color" />
        <attr name="hasBottomViewLine" format="boolean" />
    </declare-styleable>
</resources>
